----------------------------------------------------------------
SELECT DISTINCT &quot;Order Date&quot; FROM Sales1_dataset LIMIT 10;
---------------------------------------------------------------
SELECT 
    strftime('%Y', &quot;Order Date&quot;) AS order_year,
    strftime('%m', &quot;Order Date&quot;) AS order_month,
    SUM(Sales) AS revenue,
    COUNT(DISTINCT &quot;Order ID&quot;) AS volume
FROM 
    Sales1_dataset
WHERE 
    strftime('%Y-%m-%d', &quot;Order Date&quot;) BETWEEN '2014-01-01' AND '2015-12-31'
GROUP BY 
    order_year,
    order_month
ORDER BY 
    order_year,
    order_month;
---------------------------------------------
SELECT 
    SUM(&quot;Sales&quot;) AS total_revenue
FROM 
    Sales1_dataset;	
------------------------------------------
SELECT 
    COUNT(DISTINCT &quot;Order ID&quot;) AS volume
FROM 
    Sales1_dataset;
---------------------------------------------
SELECT &quot;Customer ID&quot;, &quot;Customer Name&quot;,Profit
FROM Sales1_dataset
ORDER BY Profit DESC ;	

